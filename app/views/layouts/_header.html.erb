<header class="navbar navbar-fixed-top navbar-inverse">
      <div class="container">
        <%= link_to "TODOS APP", root_url, id: "logo" %>
        <nav>
          <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed"
                                          data-toggle="collapse"
                                          data-target="#bs-example-navbar-collapse-1"
                                          aria-expanded="false">
              <span class="sr-only">Toggle navigation</span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
            </button>
          </div>
          <ul class="nav navbar-nav navbar-right collapse navbar-collapse" id="bs-example-navbar-collapse-1">
            <% if logged_in? %>
              <li><%= link_to "Profile", user_path(current_user), id: "quicklink" %></li>
              <li class="dropdown">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown" id="quicklink">
                  Todos<b class="caret"></b>
                </a>
                <ul class="dropdown-menu">
                  <li><%= link_to "New todo", new_todo_url, id: "quicklink" %></li>
                  <li><%= link_to "All todos", todos_url, id: "quicklink" %></li>
                </ul>
              </li>
              <li><%= link_to "Logout", logout_path, method: :delete, id: "quicklink" %></li>
            <% else %>
              <li><%= link_to "Sign up", signup_url, id: "quicklink" %></li>
              <li><%= link_to "Log in", login_url, id: "quicklink" %></li>
            <% end %>
            <li><%= link_to "Contact", contact_url, id: "quicklink" %></li>
          </ul>
        </nav>
      </div>
    </header>